rewrite_log on;

include include/upstream.conf;

server {
    server_name $katalyst_host;
    client_max_body_size 256M;

    include include/routes.conf;
    include include/metrics.conf;

    location / {
        root /etc/nginx/html;
        error_page 404 /404;
        error_page 500 502 503 504 /50x;
    }
}
